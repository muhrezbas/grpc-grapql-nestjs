{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@google-cloud/pubsub": "^2.16.4",
    "@google-cloud/storage": "^5.8.3",
    "@grpc/proto-loader": "^0.6.4",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/graphql": "^7.9.11",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^7.6.13",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/platform-socket.io": "^8.0.6",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^8.0.6",
    "@nextnm/nestjs-mailgun": "^1.1.1",
    "apollo-server-express": "^2.25.0",
    "argon2": "^0.27.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.22.1",
    "dayjs": "^1.10.5",
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "exceljs": "^4.2.1",
    "firebase-admin": "^9.5.0",
    "google-libphonenumber": "^3.2.22",
    "graphql": "^15.5.0",
    "graphql-tools": "^8.0.0",
    "graphql-upload": "^12.0.0",
    "grpc": "^1.24.11",
    "handlebars": "^4.7.7",
    "json-2-csv": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongodb": "^3.6.4",
    "mysql2": "^2.2.5",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^6.5.0",
    "oracledb": "^5.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.2.0",
    "pushy": "^2.0.12",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sharp": "^0.28.0",
    "shortid": "^2.2.16",
    "swagger-ui-express": "^4.1.6",
    "ts-loader": "^8.0.17",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.0",
    "xlsx-populate": "^1.21.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
