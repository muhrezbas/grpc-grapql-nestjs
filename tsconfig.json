{
  "compilerOptions": {
    "resolveJsonModule": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "lib/model/*": ["libs/model/src/*"],
      "lib/shared": ["libs/shared/src"],
      "lib/shared/*": ["libs/shared/src/*"],
    }
  }
}
